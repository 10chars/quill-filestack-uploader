{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    },
    "lib": [ "dom", "esnext" ],
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false
  }
}
